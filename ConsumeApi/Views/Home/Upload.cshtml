@model ConsumeApi.ResponseModels.ProductResponseModel

@{
    ViewData["Title"] = "Upload";
}

<h1>File Upload</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Upload"asp-controller="Home" method="post"enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">Dosya seçiniz:</label>
                <input type="file" name="formFile" class="form-control" />
                
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

